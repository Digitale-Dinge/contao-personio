{
    "name": "inspiredminds/contao-personio",
    "description": "Personio API connection for Contao.",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/inspiredminds/contao-personio",
    "authors": [
        {
            "name": "INSPIRED MINDS",
            "homepage": "https://www.inspiredminds.at"
        }
    ],
    "support": {
        "issues": "https://github.com/inspiredminds/contao-personio/issues",
        "source": "https://github.com/inspiredminds/contao-personio"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=8.1",
        "contao/core-bundle": "^4.13 || ^5.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/dependency-injection": "^5.4 || ^6.4 || ^7.1",
        "symfony/framework-bundle": "^5.4 || ^6.4 || ^7.1",
        "symfony/http-client": "^5.4 || ^6.4 || ^7.1",
        "symfony/http-client-contracts": "^2.0 || ^3.0",
        "symfony/http-foundation": "^5.4 || ^6.4 || ^7.1",
        "symfony/http-kernel": "^5.4 || ^6.4 || ^7.1",
        "symfony/messenger": "^5.4 || ^6.4 || ^7.1",
        "symfony/property-access": "^5.4 || ^6.4 || ^7.1",
        "symfony/property-info": "^5.4 || ^6.4 || ^7.1",
        "symfony/serializer": "^5.4 || ^6.4 || ^7.1",
        "codefog/contao-haste": "^5.2",
        "terminal42/contao-fineuploader": "^3.4",
        "symfony/filesystem": "^5.4 || ^6.4 || ^7.1",
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoPersonio\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoPersonio\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^6.0",
        "contao/rector": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao-community-alliance/composer-plugin": true
        }
    }
}
